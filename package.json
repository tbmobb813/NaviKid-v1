{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node\" expo start",
    "preinstall": "node ./scripts/check-no-maps.cjs",
    "start:clean": "rimraf node_modules/.cache .expo && expo start --clear",
    "start:web": "expo start --web",
    "start:ios": "expo start --ios",
    "start:android": "expo start --android",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:logic": "jest --roots ./__tests__",
    "test:logic:watch": "jest --roots ./__tests__ --watch",
    "typecheck": "tsc --noEmit",
    "test:integration": "jest __tests__/",
    "test:integration:watch": "jest --watch __tests__/",
    "test:backend": "ts-node scripts/test-backend-connection.ts",
    "test:backend-integration": "jest __tests__/integration/backend-integration.test.ts",
    "test:routing": "jest __tests__/routing-integration.test.ts",
    "test:offline": "jest __tests__/offline-validation.test.ts",
    "test:monitoring": "jest __tests__/monitoring.test.ts",
    "dev:backend": "cd backend && npm run dev",
    "test:server": "jest --config=jest.server.config.cjs",
    "test:all": "npm test && npm run test:server",
    "test:fast": "npm run test:logic",
    "test:full": "npm run test:logic && jest __tests__",
    "test:concurrent": "bash ./scripts/run-tests-concurrent.sh",
  "lint": "node ./scripts/lint-with-timeout.cjs",
  "lint:timed": "node ./scripts/lint-with-timeout.cjs",
  "lint:fast": "LINT_TIMEOUT_MS=60000 node ./scripts/lint-with-timeout.cjs --rule 'prettier/prettier:0' --rule 'react/jsx-filename-extension:0'",
  "lint:full": "LINT_TIMEOUT_MS=180000 node ./scripts/lint-with-timeout.cjs",
  "format:check": "npx prettier --check \"**/*.{ts,tsx,json,md}\"",
  "format:check:ci": "npx prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "perf-check": "node ./tools/perfCheck.cjs",
    "demo:routing": "expo start --dev-client",
    "demo:offline": "node demo-offline-monitoring.js",
    "demo:integration": "node demo-integration.js",
    "check:no-maps": "node ./scripts/check-no-maps.cjs",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:android:dev": "eas build --platform android --profile development",
    "check:native-view-managers": "adb logcat -s NativeCheck:I *:S",
    "gradle": "./gradlew",
    "gradle:tasks": "./gradlew tasks",
    "gradle:dependencies": "./gradlew dependencies",
    "gradle:dependencyUpdates": "./gradlew dependencyUpdates",
    "gradle:clean": "./gradlew clean",
    "gradle:build": "./gradlew build",
  "lint:progress": "LINT_TIMEOUT_MS=30000 node scripts/lint-perfile.cjs",
  "lint:single": "node scripts/run-lint.cjs",
  "typecheck:progress": "node scripts/run-typecheck.cjs"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@maplibre/maplibre-react-native": "^10.2.1",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.19",
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-query": "^5.90.5",
    "expo": "^54.0.13",
    "expo-application": "~7.0.7",
    "expo-audio": "^1.0.13",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "^6.0.15",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-modules-autolinking": "^3.0.15",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.12",
    "expo-secure-store": "~15.0.7",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-task-manager": "~14.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "glob": "^9.3.1",
    "gtfs-realtime-bindings": "^1.1.1",
    "lucide-react-native": "^0.544.0",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^4.0.0",
    "react-native-nitro-modules": "^0.5.0",
    "react-native-reanimated": "4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.1",
    "uuid": "^9.0.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@expo/config": "^12.0.10",
    "@expo/ngrok": "^4.1.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/metro-config": "^0.82.1",
    "@testing-library/react-native": "^12.9.0",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.21",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@xmldom/xmldom": "^0.8.4",
    "actionlint": "^2.0.6",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "csv-parse": "^5.4.0",
    "eslint": "^9.38.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^13.0.0",
    "lru-cache": "^7.16.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prom-client": "^15.0.0",
    "react-test-renderer": "^19.1.0",
    "rimraf": "^4.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true,
  "overrides": {}
}
