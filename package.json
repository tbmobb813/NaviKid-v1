{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node\" expo start",
    "preinstall": "node ./scripts/check-node-version.cjs && node ./scripts/check-no-maps.cjs",
    "start:clean": "rimraf node_modules/.cache .expo && expo start --clear",
    "start:web": "expo start --web",
    "start:ios": "expo start --ios",
    "start:android": "expo start --android",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:logic": "jest --roots ./__tests__",
    "test:logic:watch": "jest --roots ./__tests__ --watch",
    "typecheck": "tsc --noEmit",
    "test:integration": "jest __tests__/",
    "test:integration:watch": "jest --watch __tests__/",
    "test:backend": "ts-node scripts/test-backend-connection.ts",
    "test:backend-integration": "jest __tests__/integration/backend-integration.test.ts",
    "test:routing": "jest __tests__/routing-integration.test.ts",
    "test:offline": "jest __tests__/offline-validation.test.ts",
    "test:monitoring": "jest __tests__/monitoring.test.ts",
    "dev:backend": "cd backend && npm run dev",
    "test:server": "jest --config=jest.server.config.cjs",
    "test:all": "npm test && npm run test:server",
    "test:fast": "npm run test:logic",
    "test:full": "npm run test:logic && jest __tests__",
    "migrate": "prisma migrate dev",
    "test:integration:local": "bash ./scripts/run-integration-local.sh",
    "test:concurrent": "bash ./scripts/run-tests-concurrent.sh",
    "lint": "npm run lint:all",
    "lint:frontend": "eslint_d app components hooks utils --ext .ts,.tsx --cache --cache-location .cache/eslint/frontend --max-warnings=0 --config eslint.config.frontend.cjs",
    "lint:frontend:fast": "eslint_d app components hooks --ext .ts,.tsx --cache --cache-location .cache/eslint/frontend-fast --max-warnings=0 --config eslint.config.frontend.cjs",
    "lint:backend": "npm --prefix backend run lint",
    "lint:timed": "node ./scripts/lint-with-timeout.cjs",
    "lint:all": "npm run lint:frontend:fast && npm run lint:backend",
    "lint:fast": "LINT_TIMEOUT_MS=60000 node ./scripts/lint-with-timeout.cjs --rule 'prettier/prettier:0' --rule 'react/jsx-filename-extension:0'",
    "lint:full": "LINT_TIMEOUT_MS=180000 node ./scripts/lint-with-timeout.cjs",
    "lint:debug": "node tools/lint-debug.cjs",
    "format:check": "npx prettier --check \"**/*.{ts,tsx,json,md}\"",
    "format:check:ci": "npx prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "perf-check": "node ./tools/perfCheck.cjs",
    "demo:routing": "expo start --dev-client",
    "demo:offline": "node demo-offline-monitoring.js",
    "demo:integration": "node demo-integration.js",
    "check:no-maps": "node ./scripts/check-no-maps.cjs",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:android:dev": "eas build --platform android --profile development",
    "check:native-view-managers": "adb logcat -s NativeCheck:I *:S",
    "gradle": "./gradlew",
    "gradle:tasks": "./gradlew tasks",
    "gradle:dependencies": "./gradlew dependencies",
    "gradle:dependencyUpdates": "./gradlew dependencyUpdates",
    "gradle:clean": "./gradlew clean",
    "gradle:build": "./gradlew build",
    "lint:progress": "LINT_TIMEOUT_MS=30000 node scripts/lint-perfile.cjs",
    "lint:single": "node scripts/run-lint.cjs",
    "typecheck:progress": "node scripts/run-typecheck.cjs",
    "build": "tsc"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@fastify/websocket": "^11.2.0",
    "@gorhom/bottom-sheet": "^5.2.7",
    "@maplibre/maplibre-react-native": "^10.4.1",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.22",
    "@sentry/react-native": "^7.7.0",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.11",
    "expo": "^54.0.25",
    "expo-application": "~7.0.7",
    "expo-audio": "^1.0.15",
    "expo-battery": "^10.0.7",
    "expo-blur": "~15.0.7",
    "expo-camera": "^17.0.9",
    "expo-constants": "^18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "^6.0.18",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "^3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-location": "~19.0.7",
    "expo-modules-autolinking": "^3.0.22",
    "expo-notifications": "~0.32.13",
    "expo-router": "^6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "^6.0.8",
    "expo-task-manager": "^14.0.8",
    "expo-updates": "~29.0.13",
    "expo-web-browser": "^15.0.9",
    "glob": "^11.1.0",
    "gtfs-realtime-bindings": "^1.1.1",
    "lucide-react-native": "^0.555.0",
    "nativewind": "^4.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "^0.82.1",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-mmkv": "^4.0.1",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "^15.15.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.2",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "^0.6.1",
    "tmp": "^0.2.5",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@expo/config": "^12.0.10",
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/metro-config": "^0.82.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@xmldom/xmldom": "^0.8.11",
    "actionlint": "^2.0.6",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "csv-parse": "^6.1.0",
    "eslint": "^9.39.1",
    "eslint_d": "^14.3.0",
    "eslint-formatter-unix": "^9.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "express": "^5.1.0",
    "husky": "^8.0.3",
    "ioredis": "^5.8.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^14.0.1",
    "lru-cache": "^11.2.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "prom-client": "^15.1.3",
    "react-test-renderer": "^19.2.0",
    "rimraf": "^6.1.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "prepare": "husky install",
  "lint-staged": {
    "app/**/*.{ts,tsx},components/**/*.{ts,tsx},hooks/**/*.{ts,tsx},utils/**/*.{ts,tsx}": [
      "eslint_d --cache --cache-location .cache/eslint/frontend-fast --config eslint.config.frontend.cjs --max-warnings=0"
    ],
    "backend/src/**/*.{ts,tsx}": [
      "eslint_d --cache --cache-location .cache/eslint/backend-fast --ext .ts,.tsx --config backend/.eslintrc.cjs"
    ]
  },
  "private": true
}
